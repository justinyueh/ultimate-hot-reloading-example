{
  "name": "ultimate-hot-reloading-example",
  "version": "1.0.0",
  "private": true,
  "description": "React example",
  "author": "Glen Mailer <glen@stainlessed.co.uk>",
  "license": "MIT",
  "scripts": {
    "dev": "babel-node src/server/server.js",
    "start": "node build/server/server.js",
    "build": "rm -rf dist && rm -rf build && npm run build:client && npm run build:ssr && npm run build:server",
    "build:client": "NODE_ENV=production webpack-cli --config ./src/webpack/webpack.config.babel.js --mode production",
    "build:ssr": "NODE_ENV=production webpack-cli --config ./src/webpack/webpack.server.config.babel.js --mode production",
    "build:server": "babel src --out-dir build --copy-files --ignore client",
    "lint": "eslint ."
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chokidar": "^2.0.2",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.7.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "webpack-cli": "^2.0.12"
  }
}
